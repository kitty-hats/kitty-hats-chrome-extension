{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": false,
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "indent": [true, "tabs", 2],
        "object-literal-sort-keys": false,
        "no-unused-variable": true,
        "no-unused-expression": true
    },
    "rulesDirectory": []
}
